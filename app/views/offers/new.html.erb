<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-4 col-md-offset-4">

      <%= simple_form_for(@offer) do |f| %>
        <%= f.error_notification %>
        <%= f.input :city, :label => "Address"   %>
        <%= f.input :price %>
        <%= f.input :title %>
        <%= f.input :description, as: :text, :input_html => {:rows => 10} %>
        <%= f.input :rules, as: :text, :input_html => {:rows => 10} %>
        <%= f.input :photo, as: :attachinary %>

        <%= f.button :submit %>
      <% end %>


    </div>
  </div>
</div>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyBB15XgThscnBixvAWkEblUzNiaQJW23kc"></script>
<% end %>
